i = "2 3 6 8 9"
numeros = i.split()
lista = []
def converte_entrada(numeros):
    for numero in numeros:

        lista.append(int(numero))
    return lista
lista = converte_entrada(numeros)
print(lista)

i = [1, 3, 8, 9]
val1 = 0
val2 = len(i)

def processa_numeros(numeros):
    i = numeros
for numeros in i:
    val1 += int(numeros)
print(val1)
print(val2)


